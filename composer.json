{
  "name": "lukewaite/laravel-aws-cache-adapter",
  "description": "Laravel Cache Adapter for AWS Credential Caching. Allows you to reduce calls to the ec2 metadata api.",
  "keywords": [
    "laravel",
    "aws",
    "iam",
    "cache",
    "credential"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LukeWaite\\LaravelAwsCacheAdapter\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "aws/aws-sdk-php": "^3.20",
    "illuminate/support": "^10|^11"
  },
  "require-dev": {
    "laravel/pint": "^1.15",
    "mockery/mockery": "^1.4.4",
    "laravel/framework": "^10|^11",
    "phpunit/phpunit": "^10.0",
    "squizlabs/php_codesniffer": "^2.8",
    "scrutinizer/ocular": "^1.9",
    "symfony/console": "^6.0"
  },
  "scripts": {
    "cs": "phpcs --standard=psr2 src/",
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "LukeWaite\\LaravelAwsCacheAdapter\\ServiceProvider"
      ]
    }
  }
}
